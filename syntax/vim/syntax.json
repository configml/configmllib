{
	"config": {
		"eol": "\n",
		"filename": ".vimrc",
		"comment": {
			"prefix": "\" ",
			"suffix": ""
		}
	},
	"types": {
		"set": {
			"format": "{type} {name}={value}"
		},
		"execute": {
			"format": "{type} {value}"
		},
		"switch": {
			"format": "{name} {value}"
		},
		"map": {
			"format": "{type} {name} {value}"
		},
		"let": {
			"ref": "set"
		}
	}
}

